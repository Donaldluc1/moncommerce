{
  "name": "gestion-commerce-backend",
  "version": "1.0.0",
  "description": "API Backend pour application de gestion de commerce",
  "main": "src/server.js",
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test:db": "node test-prisma.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset",
    "prisma:status": "prisma migrate status",
    "db:seed": "node prisma/seed.js",
    "setup": "npm install && npx prisma generate && npx prisma migrate dev --name init && node test-prisma.js"
  },
  "keywords": ["gestion", "commerce", "api"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "@prisma/client": "^5.7.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-validator": "^7.0.1",
    "@anthropic-ai/sdk": "^0.20.0",
    "openai": "^4.28.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "prisma": "^5.7.0"
  }
}